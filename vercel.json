{
  "routes": [
    {
      "src": "/compress/([^/]+)",
      "dest": "/index.html",
      "headers": {
        "Link": "</compress/$1>; rel=canonical"
      }
    },
    {
      "src": "/discord",
      "dest": "/index.html?target=discord",
      "headers": {
        "Cache-Control": "public, max-age=3600"
      }
    }
  ],
  "cleanUrls": true
}
